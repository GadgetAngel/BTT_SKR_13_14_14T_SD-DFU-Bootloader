  87:LPC17xxLib/src/lpc17xx_i2s.c ****  * @brief		Get I2S channel value
  88:LPC17xxLib/src/lpc17xx_i2s.c ****  * @param[in]	I2Sx I2S peripheral selected, should be: LPC_I2S
  89:LPC17xxLib/src/lpc17xx_i2s.c ****  * @param[in]	TRMode is the I2S mode, should be:
  90:LPC17xxLib/src/lpc17xx_i2s.c ****  * 				- I2S_TX_MODE = 0: transmit mode
  91:LPC17xxLib/src/lpc17xx_i2s.c ****  * 				- I2S_RX_MODE = 1: receive mode
  92:LPC17xxLib/src/lpc17xx_i2s.c ****  * @return 		The channel value, should be: 1(mono) or 2(stereo)
  93:LPC17xxLib/src/lpc17xx_i2s.c ****  *********************************************************************/
  94:LPC17xxLib/src/lpc17xx_i2s.c **** static uint8_t i2s_GetChannel(LPC_I2S_TypeDef *I2Sx, uint8_t TRMode) {
  95:LPC17xxLib/src/lpc17xx_i2s.c **** 	uint8_t value;
  96:LPC17xxLib/src/lpc17xx_i2s.c **** 
  97:LPC17xxLib/src/lpc17xx_i2s.c **** 	CHECK_PARAM(PARAM_I2Sx(I2Sx));
  98:LPC17xxLib/src/lpc17xx_i2s.c **** 	CHECK_PARAM(PARAM_I2S_TRX(TRMode));
  99:LPC17xxLib/src/lpc17xx_i2s.c **** 
 100:LPC17xxLib/src/lpc17xx_i2s.c **** 	if (TRMode == I2S_TX_MODE) {
 101:LPC17xxLib/src/lpc17xx_i2s.c **** 		value = ((I2Sx->I2SDAO) & 0x04)>>2; /* get bit[2] */
 102:LPC17xxLib/src/lpc17xx_i2s.c **** 	} else {
 103:LPC17xxLib/src/lpc17xx_i2s.c **** 		value = ((I2Sx->I2SDAI) & 0x04)>>2; /* get bit[2] */
 104:LPC17xxLib/src/lpc17xx_i2s.c **** 	}
 105:LPC17xxLib/src/lpc17xx_i2s.c ****         if(value == I2S_MONO) return 1;
 106:LPC17xxLib/src/lpc17xx_i2s.c ****           return 2;
 107:LPC17xxLib/src/lpc17xx_i2s.c **** }
