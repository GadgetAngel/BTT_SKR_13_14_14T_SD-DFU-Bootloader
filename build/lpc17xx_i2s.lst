  58:LPC17xxLib/src/lpc17xx_i2s.c ****  * @brief		Get I2S wordwidth value
  59:LPC17xxLib/src/lpc17xx_i2s.c ****  * @param[in]	I2Sx I2S peripheral selected, should be: LPC_I2S
  60:LPC17xxLib/src/lpc17xx_i2s.c ****  * @param[in]	TRMode is the I2S mode, should be:
  61:LPC17xxLib/src/lpc17xx_i2s.c ****  * 				- I2S_TX_MODE = 0: transmit mode
  62:LPC17xxLib/src/lpc17xx_i2s.c ****  * 				- I2S_RX_MODE = 1: receive mode
  63:LPC17xxLib/src/lpc17xx_i2s.c ****  * @return 		The wordwidth value, should be: 8,16 or 32
  64:LPC17xxLib/src/lpc17xx_i2s.c ****  *********************************************************************/
  65:LPC17xxLib/src/lpc17xx_i2s.c **** static uint8_t i2s_GetWordWidth(LPC_I2S_TypeDef *I2Sx, uint8_t TRMode) {
  66:LPC17xxLib/src/lpc17xx_i2s.c **** 	uint8_t value;
  67:LPC17xxLib/src/lpc17xx_i2s.c **** 
  68:LPC17xxLib/src/lpc17xx_i2s.c **** 	CHECK_PARAM(PARAM_I2Sx(I2Sx));
  69:LPC17xxLib/src/lpc17xx_i2s.c **** 	CHECK_PARAM(PARAM_I2S_TRX(TRMode));
  70:LPC17xxLib/src/lpc17xx_i2s.c **** 
  71:LPC17xxLib/src/lpc17xx_i2s.c **** 	if (TRMode == I2S_TX_MODE) {
  72:LPC17xxLib/src/lpc17xx_i2s.c **** 		value = (I2Sx->I2SDAO) & 0x03; /* get wordwidth bit */
  73:LPC17xxLib/src/lpc17xx_i2s.c **** 	} else {
  74:LPC17xxLib/src/lpc17xx_i2s.c **** 		value = (I2Sx->I2SDAI) & 0x03; /* get wordwidth bit */
  75:LPC17xxLib/src/lpc17xx_i2s.c **** 	}
  76:LPC17xxLib/src/lpc17xx_i2s.c **** 	switch (value) {
  77:LPC17xxLib/src/lpc17xx_i2s.c **** 	case I2S_WORDWIDTH_8:
  78:LPC17xxLib/src/lpc17xx_i2s.c **** 		return 8;
  79:LPC17xxLib/src/lpc17xx_i2s.c **** 	case I2S_WORDWIDTH_16:
  80:LPC17xxLib/src/lpc17xx_i2s.c **** 		return 16;
  81:LPC17xxLib/src/lpc17xx_i2s.c **** 	default:
  82:LPC17xxLib/src/lpc17xx_i2s.c **** 		return 32;
  83:LPC17xxLib/src/lpc17xx_i2s.c **** 	}
  84:LPC17xxLib/src/lpc17xx_i2s.c **** }
  85:LPC17xxLib/src/lpc17xx_i2s.c **** 
